@media (min-width: 350px) and (max-width: 1400px)
    .first
        &__links
            margin: 15px auto 0
    .first,
    .market,
    .contact,
    .second,
    .question
        &::after
            display: block
            height: 450px

    .market
        padding-bottom: 100px
    .question
        padding-top: 0

